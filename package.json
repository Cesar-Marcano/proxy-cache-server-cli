{
  "name": "redis-cache-proxy",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "redis-cache-proxy": "dist/index.js"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "start:ts-node": "ts-node ./src/index.ts",
    "start:dev": "ts-node ./src/index.ts --port 3000 --origin dummyjson.com",
    "build": "tsc",
    "package": "pkg . --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output ./build/redis-cache-proxy --compress GZip",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.4",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "commander": "^13.1.0",
    "express": "^4.21.2",
    "ioredis": "^5.6.0"
  },
  "pkg": {
    "assets": [
      "node_modules/axios/**/*",
      "node_modules/express/**/*",
      "node_modules/ioredis/**/*"
    ]
  }
